datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}



model Visitor {
  id          Int      @id @default(autoincrement())
  ip_address  String
  user_agent  String
  page_url    String
  referrer    String
  device_type String
  browser     String
  os          String
  country     String
  region      String
  city        String
  createdAt   DateTime @default(now())
}
